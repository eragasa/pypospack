# ---- define interatomic potential
include potential.mod
mass 1 58.6934

group Ni type 1
32 atoms in group Ni

pair_style eam/alloy
pair_coeff * * /Users/eugeneragasa/repos/pypospack/tests/tests_integration/qoi/StackingFaultEnergyCalculation/rank_test/Ni_fcc_usf.lmps_min_sf/Ni.eam.alloy Ni

