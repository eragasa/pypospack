# ---- define interatomic potential
include potential.mod
mass 1 58.6934

group Ni type 1
10 atoms in group Ni

pair_style eam/alloy
pair_coeff * * /home/sullberg/repos/pypospack/examples/Ni__eam__born_exp_rose/01_preconditioning_3.5NN/rank_19/Ni_fcc_110_s.lmps_min_pos/Ni.eam.alloy Ni

